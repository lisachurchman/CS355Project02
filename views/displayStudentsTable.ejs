<% include header %>

    <div>
    <% if(rs.length > 0) { %>
        <table border="1">
            <tr><th>Name</th></tr>
            <% for (var i=0; rs.length > i; i++) { %>
                <tr>
                 <td><a href="/student/?studentid=<%= rs[i].Student_number %>"> <%= rs[i].Name %></a></td>
                </tr>
            <% } %>
        </table>
    <% } else { %>
         No students are entered into the Student database table.
    <% } %>
    </div>

    <div style="margin-top:10px;">
        <a href="/student/create">Create New Student</a>
    </div>

<% include footer %>